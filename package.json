{
  "name": "riotjs-webpack",
  "version": "0.0.1",
  "description": "A nice webpack riotjs boilerplate with performance in mind",
  "repository": "",
  "main": "app/main.js",
  "author": "Pachito Marco Calabrese <pm.calabrese@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --watch --hot --colors -d --port 3000",
    "build": "webpack",
    "build-serve": "npm run build && npm run serve",
    "serve": "live-server --port=4200 --host=localhost --entry-file=/public/index.html",
    "sw": "sw-precache --config=sw-precache-config.js"
  },
  "keywords": [
    "riot",
    "webpack"
  ],
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-autobind-class-methods": "^5.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-riot": "^1.1.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "live-server": "^1.2.0",
    "riot-hot-reload": "1.0.0",
    "riot-tag-loader": "1.0.0",
    "sw-precache": "^5.2.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "bulma": "^0.5.0",
    "lodash": "^4.17.4",
    "riot": "^3.6.1",
    "riot-route": "^3.1.2",
    "systemjs": "^0.20.17"
  }
}
